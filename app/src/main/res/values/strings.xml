    <string name="view_active_measurements_speedtest_title">Prueba de Velocidad</string>
    <string name="view_active_measurements_speedtest_description">Esta prueba permite determinar la velocidad efectiva de tu conexión a internet para subir y bajar datos. El resultado permite identificar qué tan estable y rápida es tu conexión.</string>
    <string name="view_active_measurements_speedtest_upload_speed">Velocidad de Subida</string>
