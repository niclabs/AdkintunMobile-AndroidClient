<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:key="@string/settings_main_screen_key">

    <PreferenceCategory
        android:key="@string/settings_video_test_category_key"
        android:title="@string/settings_video_test_category_title"
        android:summary="@string/view_active_measurements_mediatest_description"
        android:layout="@layout/preference_category_active_measurements"
        />
    <PreferenceCategory
        android:key="@string/settings_video_test_quality_category_key"
        android:title="@string/settings_video_test_quality_category_title"
        >
        <CheckBoxPreference
            android:key="@string/settings_video_test_quality_tiny_key"
            android:title="@string/settings_video_test_quality_tiny_pixels"
            android:defaultValue="false"/>
        <CheckBoxPreference
            android:key="@string/settings_video_test_quality_small_key"
            android:title="@string/settings_video_test_quality_small_pixels"
            android:defaultValue="false"/>
        <CheckBoxPreference
            android:key="@string/settings_video_test_quality_medium_key"
            android:title="@string/settings_video_test_quality_medium_pixels"
            android:defaultValue="false"/>
        <CheckBoxPreference
            android:key="@string/settings_video_test_quality_large_key"
            android:title="@string/settings_video_test_quality_large_pixels"
            android:defaultValue="false"/>
        <CheckBoxPreference
            android:key="@string/settings_video_test_quality_hd720_key"
            android:title="@string/settings_video_test_quality_hd720_pixels"
            android:defaultValue="false"/>
        <Preference
            android:key="@string/settings_video_test_max_quality_key"
            />
    </PreferenceCategory>
